{
    "name": "michaelesmith/front-controller-security-bundle",
    "description": "Allows for easy ip based security for front controllers in Symfony2",
    "keywords": ["security", "front controller"],
    "homepage": "http://michaelsmithonline.blogspot.com/p/front-controller-security-bundle.html",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Michael Smith",
            "email": "sitecross@gmail.com",
            "homepage": "http://michaelsmithonline.blogspot.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "mikey179/vfsStream": "v1.1.0"
    },
    "autoload": {
        "psr-0": { "MS\\Bundle\\FrontControllerSecurityBundle": "" }
    },
    "target-dir": "MS/Bundle/FrontControllerSecurityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
